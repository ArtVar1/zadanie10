package com.company;

public class kwadrat implements Figura{
    {
    int bok;

    public kwadrat(int bok)
    {
        this.bok = bok;
    }
        @Override
    public void obliczObwod()
    {
        System.out.println(4*bok);
    }
    @Override
    public void obliczPole()
    {
        System.out.println(bok*bok);
    }

}
